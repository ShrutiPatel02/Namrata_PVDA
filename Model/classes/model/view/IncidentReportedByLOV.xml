<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="IncidentReportedByLOV"
  Version="11.1.1.65.57"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.view.IncidentReportedByLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_managerId"
    Kind="where"
    Type="oracle.jbo.domain.Number">
    <TransientExpression><![CDATA[adf.userSession.userData.loginUserId]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT EMPLOYEEID,FIRSTNAME ||' '|| LASTNAME AS FULLNAME FROM EMPLOYEE WHERE MANAGERID = :p_managerId  
UNION  
SELECT emp1.EMPLOYEEID,emp1.FIRSTNAME ||' '|| emp1.LASTNAME FROM EMPLOYEE emp,INCIDENTREPORT inc,EMPLOYEE emp1  
WHERE inc.hrid = emp.EMPLOYEEID  
AND inc.EMPLOYEEID = emp1.employeeid  
AND inc.hrid = :p_managerId]]>
  </SQLQuery>
  <ViewAttribute
    Name="Employeeid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="EMPLOYEEID"
    Expression="EMPLOYEEID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="11"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Fullname"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="101"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FULLNAME"
    Expression="FULLNAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="101"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
